{
  "name": "abacus",
  "version": "0.0.1",
  "author": "Automattic Inc.",
  "description": "Automattic's Experimentation Platform UI",
  "license": "GPL-2.0-or-later",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Automattic/abacus.git"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@rehooks/component-size": "^1.0.3",
    "@types/mathjs": "^6.0.11",
    "@types/react-dom": "*",
    "@types/react-plotly.js": "^2.2.4",
    "@types/react-router-dom": "^5.1.6",
    "ag-grid-community": "^25.1.0",
    "ag-grid-react": "^25.1.0",
    "chi-squared-test": "^1.1.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.13.0",
    "debug": "^4.1.1",
    "formik": "^2.1.4",
    "formik-material-ui": "^3.0.0",
    "formik-material-ui-lab": "0.0.5",
    "http-status-codes": "^2.1.4",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.21",
    "material-table": "^1.59.0",
    "mathjs": "^9.3.2",
    "normalizr": "^3.6.0",
    "norminv": "^2.0.0",
    "notistack": "^1.0.0",
    "plotly.js": "^1.55.2",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-plotly.js": "^2.5.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "ts-pattern": "^4.1.3",
    "wretch": "^1.7.4",
    "yup": "^0.29.1"
  },
  "@comment devDependencies": {
    "@package isomorphic-fetch": "Added to make `fetch` available in integration tests. Hence the reason it is a dev dependency."
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/css-mediaquery": "^0.1.0",
    "@types/debug": "^4.1.5",
    "@types/expect-puppeteer": "^4.4.1",
    "@types/jest": "^27.5.2",
    "@types/jest-environment-puppeteer": "^5.0.3",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.11.2",
    "@types/yup": "^0.29.14",
    "babel-plugin-remove-debug": "0.0.4",
    "cross-env": "^7.0.2",
    "css-mediaquery": "^0.1.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "fetch-mock-jest": "^1.5.1",
    "full-icu": "^1.3.1",
    "husky": "^4.3.8",
    "isomorphic-fetch": "^3.0.0",
    "jest-environment-puppeteer": "^8.0.6",
    "lint-staged": "^10.1.7",
    "mockdate": "^3.0.2",
    "prettier": "^2.0.5",
    "puppeteer": "^19.11.1",
    "react-test-renderer": "^16.13.1",
    "serve": "^11.3.2",
    "typescript": "4.9.5",
    "web-vitals": "^2.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.*": "prettier --write",
    "*.{ts,tsx}": "eslint --fix"
  },
  "scripts": {
    "build": "react-scripts --max_old_space_size=4096 build",
    "dev": "react-scripts --max_old_space_size=4096 start",
    "docker-build": "docker build -t abacus .",
    "docker-start": "docker run -p 8888:3000 abacus:latest",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "@comment jest": "Run jest with consistent TZ and LANG",
    "jest": "cross-env NODE_ICU_DATA=node_modules/full-icu TZ=America/New_York LANG=en-GB jest",
    "lint": "npm run lint:ts",
    "lint:ts": "eslint . --ext .ts,.tsx",
    "lint:ts:fix": "eslint . --ext .ts,.tsx --fix",
    "start": "serve -s build -l 3000",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "@comment test:e2e": "Runs the e2e tests. It will spawn Chromium to run the tests.",
    "test:e2e": "npm run jest -- --config=jest.e2e.config.js --runInBand",
    "test:e2e:smoke": "npm run jest -- --config=jest.e2e.config.js --runInBand e2e/smoke.test.tsx",
    "test:integration": "npm run jest -- --config=jest.integration.config.js --runInBand",
    "test:unit": "npm run jest",
    "type-check": "tsc",
    "verify": "npm run format:check && npm run lint && npm run test:all"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "16.18.1",
    "npm": "8.19.2"
  }
}
